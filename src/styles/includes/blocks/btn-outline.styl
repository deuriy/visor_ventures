.btn-outline
	display inline-flex
	padding 16px 61px
	border 1px solid #fff
	outline none
	border-radius 10px
	background none
	color #FFF
	font-weight 600
	font-size 14px
	line-height 1.5
	font-family var(--base-font)
	text-decoration none
	@extend .transition
	
	&:hover
		background #fff
		color #262626